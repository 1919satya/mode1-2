package com.hcl.collections;

import java.util.Comparator;
/**
 * main.
 * @author coalesce sai.
 *
 */

public class BasicComparator implements Comparator {
   /**
    * basic compare.
    */

  @Override
   public int compare(Object o1, Object o2) {
    /**
     * comparing function.
     */
    Employ e1 = (Employ)o1;
    Employ e2 = (Employ)o2;
      if (e1.basic >= e2.basic){
        return -1;
		} else {
			return 1;
		}
		
	}

}
