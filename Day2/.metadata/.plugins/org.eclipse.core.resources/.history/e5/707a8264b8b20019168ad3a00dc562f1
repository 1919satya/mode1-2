package p1;

public class Demo {

	private int x=12;
	public String name="Satya Sai";
	protected String city="Bangalore";
	double basic=85347.55;
	
	public void show() {
		Demo obj=new Demo(); 
		System.out.println(obj.x);
		System.out.println(obj.name);
		System.out.println(obj.city);
		System.out.println(obj.basic);
	}
}
