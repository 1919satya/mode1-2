package com.hcl.inheritance;

public class Custom {
		public static void main(String[] args) {
		String str="Satya Sai";
		String str2="Satya Sai";
		System.out.println("length of string"+str.length());
		System.out.println("character of string"+str.charAt(4));
		System.out.println("middle name"+str.substring(2,4));
		System.out.println("Equality of name "+str.equals(str2));
		System.out.println("lower case of name"+str2.toLowerCase());
		System.out.println("upper case of name"+str.toUpperCase());
	}

}
