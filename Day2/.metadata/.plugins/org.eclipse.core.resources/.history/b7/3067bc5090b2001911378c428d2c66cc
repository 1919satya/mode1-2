package com.hcl.Boxing;

public class EmploySearch {
	
	public Employ showEmploy(int empno){
		Employ employ=null;
		if(empno==1){
			employ =new Employ();
			employ.empno=100;
			employ.name="satya";
			employ.basic=8821;
		}
		if(empno==3){
			employ =new Employ();
			employ.empno=200;
			employ.name="pream";
			employ.basic=43516;
			
		}
		return employ;
	}
